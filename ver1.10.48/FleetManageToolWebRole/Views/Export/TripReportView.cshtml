            var td4 = $("<td></td>");
            var td5 = $("<td></td>");
            var td6 = $("<td></td>");
            var td7 = $("<td></td>");
            var td8 = $("<td></td>");
            var td9 = $("<td></td>");
            td1.append($("<div style = 'white-space:nowrap; overflow:hidden;text-overflow:ellipsis;width:100%;font-size:13px;font-family:Microsoft YaHei;'title = '" + holdjson[i].group + "'>" + holdjson[i].group + "</div>"));
